{
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/index.js" }
  ],
  "redirects": [
    { "source": "/old-route", "destination": "/new-route", "permanent": true }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
